{
  "name": "react-polygon",
  "version": "0.1.0",
  "description": "A react add-on for drawing polygons for any number of sides, as well as animation",
  "main": "./dist/Polygon.js",
  "scripts": {
    "test": "standard --verbose | snazzy && jest",
    "wpp": "webpack --mode=production",
    "wpd": "webpack --mode=development",
    "wpdw": "yarn wpd -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peteroid/react-polygon.git"
  },
  "keywords": [
    "react",
    "polygon",
    "svg",
    "drawing"
  ],
  "author": "PETEroid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peteroid/react-polygon/issues"
  },
  "homepage": "https://github.com/peteroid/react-polygon#readme",
  "dependencies": {
    "react": "^16.4.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "test",
      "expect",
      "requestAnimationFrame"
    ],
    "ignore": [
      "dist",
      "build"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.16.0",
    "jest": "^23.2.0",
    "prop-types": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
